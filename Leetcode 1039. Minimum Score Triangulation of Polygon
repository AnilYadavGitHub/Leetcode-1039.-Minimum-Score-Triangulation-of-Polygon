class Solution {
    private:
    int solve(vector<int>& values , vector<vector<int> > &dp , int i , int j){

        if(i + 1 == j)
            return 0;

        if(dp[i][j] != -1)
            return dp[i][j];
        
        int ans = INT_MAX;

        for(int k = i + 1 ; k < j ; k++){
            ans = min(ans , values[i] * values[j] * values[k] + solve(values , dp , i , k) + solve(values , dp , k , j));
        }

          return dp[i][j] = ans;
    }
public:
    int minScoreTriangulation(vector<int>& values) {
        int n = values.size();

        vector<vector<int> >dp(n, vector<int> (n , -1));

        return solve(values ,dp, 0, n - 1);
    }
};
